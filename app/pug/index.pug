doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    title Square
    link(rel="icon" href="../img/favicon.png")
    link(rel="stylesheet" href="../css/app.min.css")
  body
    header.site-header

      div.header-top-line

        div.header-hamburger-wrapper
          button.hamburger.hamburger--squeeze(type="button")
            span.hamburger-box: span.hamburger-inner

        div.header-logo-wrapper
          img.header-logo-square(src="../img/square-logo.png" alt="squere")

        div.header-social-links-wrapper
          ul.header-social-links
            li.header-social-link-item: a.header-social-link(href="#"): i.fa.fa-facebook
            li.header-social-link-item: a.header-social-link(href="#"): i.fa.fa-twitter
            li.header-social-link-item: a.header-social-link(href="#"): i.fa.fa-instagram
            li.header-social-link-item: a.header-social-link(href="#"): i.fa.fa-flickr
            li.header-social-link-item: a.header-social-link(href="#"): i.fa.fa-google-plus
            li.header-social-link-item: a.header-social-link(href="#"): i.fa.fa-envelope
